<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	
	<ui:style>
		.welcomeHeader {
			font-size: 2em;
			color: #1144ff;
			margin-left: 50px;
		}
		.subHeader { 
			font-size: 1.5em;
			color: #222277;
			margin-left: 100px;
		}
		.subHeaderText { 
			color: #998866;
			margin-left: 150px;
		}
		.text { 
			color: #998866;
			margin-left: 100px;
		}
	</ui:style>
	
	<g:HTMLPanel>
	<div class='{style.welcomeHeader}'>Dream Feature</div>
		<div class='{style.text}'>Feature made possible by dreams, unicorn smiles, and rainbows.</div>
	<div class='{style.welcomeHeader}'>Future Feature</div>
		<div class='{style.text}'>This feature will be possible if there's ever more time.</div>
	</g:HTMLPanel>

</ui:UiBinder>
	