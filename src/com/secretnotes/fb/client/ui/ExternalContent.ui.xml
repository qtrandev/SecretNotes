<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	
	<ui:with field='res' type='com.secretnotes.fb.client.ui.WelcomeResource'/>
	<ui:with field='notes' type='com.secretnotes.fb.client.NotesConstants'/>
	<ui:style field='myStyle'>
		.pretty { background-color: Skyblue; border: 1px solid yellow; color:orange }
	</ui:style>
	
	<g:HTMLPanel>
<!-- 		<g:Image resource='{res.logo}'/> -->
		
		<div class='{res.style.mainBlock}'>
			<div>
				Well hello there
				<span class='{res.style.nameSpan}' ui:field='nameSpan'/>
			</div>
		</div>
		<div>
  		What's up, <ui:text from='{notes.cute}'/>.
		</div>
	</g:HTMLPanel>
	
</ui:UiBinder>



