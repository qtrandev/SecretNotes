<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'>
	
	<ui:style>
		.welcomeHeader {
			font-size: 2em;
			color: #1144ff;
			margin-left: 50px;
		}
		.subHeader { 
			font-size: 1.5em;
			color: #222277;
			margin-left: 100px;
		}
		.subHeaderText { 
			color: #998866;
			margin-left: 150px;
		}
		.text { 
			color: #998866;
			margin-left: 100px;
		}
	</ui:style>
	
	<g:HTMLPanel>
	<div class='{style.welcomeHeader}'><span ui:field='pictureSpan'/></div>
	<div class='{style.welcomeHeader}'>ID</div>
		<div class='{style.text}'><span ui:field='idSpan'/></div>
	<div class='{style.welcomeHeader}'>FIRST NAME</div>
		<div class='{style.text}'><span ui:field='firstNameSpan'/></div>
	<div class='{style.welcomeHeader}'>LAST NAME</div>
		<div class='{style.text}'><span ui:field='lastNameSpan'/></div>
	<div class='{style.welcomeHeader}'>LINK</div>
		<div class='{style.text}'><span ui:field='linkSpan'/></div>
	<div class='{style.welcomeHeader}'>GENDER</div>
		<div class='{style.text}'><span ui:field='genderSpan'/></div>
	<div class='{style.welcomeHeader}'>TIMEZONE</div>
		<div class='{style.text}'><span ui:field='timezoneSpan'/></div>
	<div class='{style.welcomeHeader}'>LOCALE</div>
		<div class='{style.text}'><span ui:field='localeSpan'/></div>
	<div class='{style.welcomeHeader}'>VERIFIED</div>
		<div class='{style.text}'><span ui:field='verifiedSpan'/></div>
	<div class='{style.welcomeHeader}'>UPDATED TIME</div>
		<div class='{style.text}'><span ui:field='updatedTimeSpan'/></div>
	</g:HTMLPanel>

</ui:UiBinder>
	